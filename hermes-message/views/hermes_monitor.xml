<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="hermes_monitor_checknotify" model="ir.cron">
            <field name="name">Hermes Monitor: check and send mobile notify</field>
            <field name="model_id" ref="model_hermes_monitor"/>
            <field name="state">code</field>
            <field name="code">model._checkNotify()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True" />
        </record>
    </data>
</odoo>
